{
  "name": "ai-lawyer-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --ci",
    "start": "expo start",
    "start:clear": "expo start --clear",
    "start:watch": "expo start --clear",
    "dev": "expo start --clear",
    "reload": "expo start --clear",
    "ios:sim": "expo start --clear",
    "reset-cache": "rm -rf $TMPDIR/metro-* $TMPDIR/haste-map-* 2>/dev/null; watchman watch-del-all 2>/dev/null; npx expo start --clear",
    "android": "expo run:android",
    "android:emulator": "JAVA_HOME=\"/Applications/Android Studio.app/Contents/jbr/Contents/Home\" ANDROID_HOME=\"$HOME/Library/Android/sdk\" REACT_NATIVE_PACKAGER_HOSTNAME=10.0.2.2 PATH=\"/Applications/Android Studio.app/Contents/jbr/Contents/Home/bin:$HOME/Library/Android/sdk/emulator:$HOME/Library/Android/sdk/platform-tools:$PATH\" npx expo run:android",
    "android:uninstall": "adb uninstall com.anonymous.ailawyer 2>/dev/null || true",
    "android:build": "npx eas-cli build --platform android --profile production",
    "android:apk": "npx eas-cli build --platform android --profile production",
    "ios": "node scripts/run-ios-simulator.js",
    "ios:clean": "rm -rf ios/build .expo node_modules/.cache $TMPDIR/metro-* $TMPDIR/haste-map-* 2>/dev/null; watchman watch-del-all 2>/dev/null; echo 'Cache cleared. Run: npx expo run:ios'",
    "ios:uninstall": "xcrun simctl uninstall booted com.anonymous.ai-lawyer 2>/dev/null; echo 'App removed from simulator'",
    "ios:fresh": "xcrun simctl uninstall booted com.anonymous.ai-lawyer 2>/dev/null; npx expo start --clear & sleep 12 && npx expo run:ios --no-bundler; wait",
    "ios:run": "npx expo run:ios --no-bundler -d \"iPhone 17 Pro\"",
    "android:run": "npx expo run:android --no-bundler",
    "ios:dev": "npx expo run:ios",
    "ios:simulator": "node scripts/run-ios-simulator.js",
    "ios:live": "npx expo start --clear & sleep 8 && npx expo run:ios --no-bundler; wait",
    "ios:build": "npx eas-cli build --platform ios --profile production",
    "ios:submit": "npx eas-cli submit --platform ios --profile production --latest",
    "web": "expo start --web",
    "start:dev": "expo start --clear",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/metro-config": "~54.0.14",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-menu/menu": "^2.0.0",
    "@react-navigation/bottom-tabs": "^8.0.0-alpha.13",
    "@react-navigation/native": "^8.0.0-alpha.11",
    "@react-navigation/native-stack": "^8.0.0-alpha.13",
    "@supabase/supabase-js": "^2.97.0",
    "@tabler/icons-react-native": "^3.36.1",
    "base64-arraybuffer": "^1.0.2",
    "expo": "~54.0.33",
    "expo-apple-authentication": "~8.0.8",
    "expo-application": "~7.0.8",
    "expo-auth-session": "~7.0.10",
    "expo-camera": "~17.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "^14.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-intent-launcher": "~13.0.8",
    "expo-linking": "~8.0.11",
    "expo-localization": "~17.0.8",
    "expo-media-library": "~18.2.1",
    "expo-print": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-store-review": "~9.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "i18next": "^25.8.13",
    "lucide-react-native": "^0.564.0",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-i18next": "^16.5.4",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.23.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0"
  },
  "private": true,
  "devDependencies": {
    "@types/react": "~19.1.10",
    "babel-preset-expo": "~54.0.10",
    "eas-cli": "^13.0.0",
    "jest": "~29.7.0",
    "patch-package": "^8.0.0",
    "typescript": "~5.9.2"
  }
}
